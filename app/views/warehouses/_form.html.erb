    <%= f.label :name%>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :code %>
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.label :area %>
    <%= f.number_field :area %>
  </p>
  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :address %>
